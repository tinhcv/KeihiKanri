<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ki1nhom2.btl.qlct">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QLCT"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".homeState.HomeStateActivity" />
        <activity android:name=".transactionsState.TransactionStateActivity" />
        <activity android:name=".addState.AddStateActivity" />
        <activity android:name=".statsState.StatsStateActivity" />
        <activity android:name=".profileState.ProfileStateActivity" />
        <activity android:name=".profileState.SignupActivity" />
        <activity android:name=".profileState.LoginActivity" />
    </application>

</manifest>